[core]
  editor = lvim
  excludesfile = /Users/rudeigerc/.gitignore_global
[gpg]
  program = /usr/local/bin/gpg
[commit]
  gpgSign = true
[init]
  defaultBranch = main
[alias]
  co = checkout
  br = branch
  ci = commit
  st = status
  ls = log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=short
  unstage = reset HEAD --
  last = log -1 HEAD
[pull]
  rebase = true
